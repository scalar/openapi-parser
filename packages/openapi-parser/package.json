{
  "name": "@scalar/openapi-parser",
  "description": "modern OpenAPI parser written in TypeScript",
  "keywords": [
    "openapi",
    "scalar",
    "swagger",
    "parser",
    "typescript"
  ],
  "license": "MIT",
  "author": "Scalar (https://github.com/scalar)",
  "homepage": "https://github.com/scalar/scalar",
  "bugs": "https://github.com/scalar/scalar/issues/new/choose",
  "version": "0.7.1",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js"
    },
    "./plugins/fetch-urls": {
      "import": "./dist/src/utils/load/plugins/fetchUrls.js"
    },
    "./plugins/read-files": {
      "import": "./dist/src/utils/load/plugins/readFiles.js"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/scalar/openapi-parser.git",
    "directory": "packages/openapi-parser"
  },
  "scripts": {
    "types:check": "tsc --noEmit --skipLibCheck",
    "build": "pnpm types:check && rollup -c --configPlugin typescript",
    "test": "vitest"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.0",
    "glob": "^10.4.1",
    "just-diff": "^6.0.2",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-webpack-stats": "^0.2.6",
    "tinybench": "^2.8.0",
    "tslib": "^2.6.2"
  },
  "dependencies": {
    "ajv": "^8.14.0",
    "ajv-draft-04": "^1.0.0",
    "ajv-formats": "^3.0.1",
    "jsonpointer": "^5.0.1",
    "leven": "^4.0.0",
    "yaml": "^2.4.2"
  }
}
